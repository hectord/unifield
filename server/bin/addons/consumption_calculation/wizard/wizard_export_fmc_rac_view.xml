<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
    
    	<record id="wizard_export_fmc_view" model="ir.ui.view">
            <field name="name">wizard.export.fmc.view</field>
            <field name="model">wizard.export.fmc</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="FMC lines exported">
                    <separator colspan="4" string="Message" />
                    <field name="message" colspan="4" nolabel="1" />
                    <separator colspan="4" string="File to download" />
                    <field name="file" colspan="4" nolabel="1" filename="filename" />
                    <field name="filename" invisible="1"/>
                    <newline />
                    <label colspan="2" />
                    <button name="close_window" type="object" string="Close window" icon="gtk-cancel" colspan="2" />
                </form>
            </field>
        </record>
        
        <record id="wizard_export_rac_view" model="ir.ui.view">
            <field name="name">wizard.export.rac.view</field>
            <field name="model">wizard.export.rac</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="RAC lines exported">
                    <separator colspan="4" string="Message" />
                    <field name="message" colspan="4" nolabel="1" />
                    <separator colspan="4" string="File to download" />
                    <field name="file" colspan="4" nolabel="1" filename="filename" />
                    <field name="filename" invisible="1"/>
                    <newline />
                    <label colspan="2" />
                    <button name="close_window" type="object" string="Close window" icon="gtk-cancel" colspan="2" />
                </form>
            </field>
        </record>
    
    </data>
</openerp>