	</Cell>
	<Cell ss:StyleID="s31">
	<Data ss:Type="Number">${( line[6] or 0.00)|x}</Data>
	</Cell>
	<Cell ss:StyleID="s34" ss:Formula="=+RC[-2]-RC[-1]">
