</Cell>

<Cell ss:StyleID="s31">
<Data ss:Type="Number">${( line[2] or 0.00)|x}</Data>
</Cell>
<Cell ss:StyleID="s31">
<Data ss:Type="Number">${( line[3] or 0.00)|x}</Data>
</Cell>
<Cell ss:StyleID="s32" ss:Formula="=+RC[-2]-RC[-1]">
<Data ss:Type="Number"></Data>
</Cell>
