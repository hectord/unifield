<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>
		
		<record id="catalogue_import_lines_form_view" model="ir.ui.view">
			<field name="name">catalogue.import.lines.form.view</field>
			<field name="model">catalogue.import.lines</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Import lines to a catalogue">
					<separator colspan="4" string="Data" />
					<field name="catalogue_id" readonly="1" colspan="4" />
					<field name="file_to_import" colspan="4" />
					<separator colspan="4" string="Actions" />
					<button special="cancel" string="Cancel" icon="gtk-cancel" colspan="2" />
					<button name="import_file" string="Import lines" icon="gtk-execute" colspan="2" type="object" />
				</form>
			</field>
		</record>
		
	</data>
</openerp>
